<h1>Clicks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clicks.each do |click| %>
      <tr>
        <td><%= click.id %></td>
        <td><%= click.advertisement_id %></td>
        <td><%= click.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= line_chart Click.group_by_day(:created_at, last: 5).count %>

<%= line_chart Click.group(:advertisement_id).group_by_day(:created_at).count %>
